<entity class="effect" name="rockette">
    <texture mode="with_directions" path="medias/projectiles/rockette.png"/>
    <particle count="1" max="1" name="rockette" type="animated_spritesheet">
        <psize x="64" y="64"/>
        <animation>
            <frames>
                <frame n="0"/>
                <frame n="1"/>
                <frame n="1"/>
                <frame n="1"/>
                <frame n="1"/>
                <frame n="1"/>
                <frame n="1"/>
                <frame n="1"/>
            </frames>
            <duration value="100"/>
        </animation>
        <spawner type="point"/>
        <time_generator max_time="4.0" min_time="4.0"/>
        <size_generator max_end_size="64.0" max_start_size="64.0" min_end_size="64.0" min_start_size="64.0"/>
        <velocity_generator max_start_speed="80.0" min_start_speed="80.0" type="aimed"/>
        <color_generator>
            <min_start_col a="255" b="255" g="255" r="255"/>
            <max_start_col a="255" b="255" g="255" r="255"/>
            <min_end_col a="255" b="255" g="255" r="255"/>
            <max_end_col a="255" b="255" g="255" r="255"/>
        </color_generator>
    </particle>
    <sound_buffer path="medias/projectiles/rockette.flac"/>
</entity>