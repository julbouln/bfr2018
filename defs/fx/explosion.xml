<entity class="effect" name="explosion">
    <texture path="medias/extra/circleTexture.png"/>
    <particle name="explosion" count="200" max="1000" type="texture">
        <spawner type="circle" x="20" y="20"/>
        <time_generator min_time="1" max_time="1"/>
        <size_generator min_start_size="12" max_start_size="15" min_end_size="30" max_end_size="50"/>
        <velocity_generator type="angled" min_angle="-180.00002" max_angle="180.00002" min_start_speed="60" max_start_speed="90"/>
        <rotation_generator min_start_angle="-20" max_start_angle="-20" min_end_angle="90" max_end_angle="90"/>
        <euler_updater accel_x="0" accel_y="0"/>
        <color_generator>
            <min_start_col a="255" r="255" g="67" b="0"/>
            <max_start_col a="255" r="255" g="217" b="0"/>
            <min_end_col a="255" r="94" g="94" b="94"/>
            <max_end_col a="109" r="22" g="22" b="22"/>
        </color_generator>
    </particle>
    <sound_buffer path="medias/misc/explosion.flac"/>
</entity>
